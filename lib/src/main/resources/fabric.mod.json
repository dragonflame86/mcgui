{
  "schemaVersion": 1,
  "id": "mcgui",
  "version": "${version}",
  "name": "MCGui",
  "description": "A GUI library for fabric. Simple HTML like syntax and easy to use. ",
  "authors": [
    "Calum Henderson"
  ],
  "contact": {
    "homepage": "https://discord.gg/UaqnXhSdNg",
    "sources": "https://github.com/diabolical17/mcgui"
  },

  "license": "CC0-1.0",
  "icon": "assets/mcgui/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "cal.codes.mcgui.MCGuiMain"
    ],
    "server": [
      "cal.codes.mcgui.MCGuiServer"
    ],
    "client": [
      "cal.codes.mcgui.MCGuiClient"
    ]
  },
  "mixins": [
    "mcgui.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  }
}